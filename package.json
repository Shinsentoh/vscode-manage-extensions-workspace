{
	"name": "vscode-manage-extensions-workspaces",
	"displayName": "MEW - Manage your Extensions by Workspace",
	"description": "Create your vscode extensions bundles, then select one or many to be used in a workspace.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"vscode-settings",
		"code-settings",
		"profile",
		"extensions",
		"extension",
		"manager",
		"workspace"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Shinsentoh/vscode-manage-extensions-workspace"
	},
	"author": {
		"name": "Shinsentoh",
		"url": "https://github.com/Shinsentoh"
	},
	"publisher": "Shinsentoh",
	"activationEvents": [
		"onCommand:vscode-manage-extensions-workspaces.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-manage-extensions-workspaces.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"version": "auto-changelog -p && git add CHANGELOG.md",
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "jest",
		"ui-test": "node ./test/test-runner"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/jest": "^26.0.24",
		"@types/jest-cli": "^24.3.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.58.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"auto-changelog": "^2.3.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"jest": "^27.0.6",
		"jest-cli": "^27.0.6",
		"ts-jest": "^27.0.4",
		"ts-node": "^10.1.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {}
}
